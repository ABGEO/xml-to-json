    "XML",
    "JSON",
    "Converter",
    "XMLtoJSON"
  ],
  "homepage": "https://github.com/ABGEO/xml-to-json",
  "license": "MIT",
  "authors": [
    {
      "name": "Temuri Takalandze",
      "email": "takalandzet@gmail.com",
